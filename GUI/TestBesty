-- Quantum 9.0 - Vaprux Interface
-- Minimal purple UI inspired by screenshot

repeat task.wait() until game:GetService("Players").LocalPlayer

local CoreGui = game:GetService("CoreGui")
if CoreGui:FindFirstChild("QuantumUI") then CoreGui.QuantumUI:Destroy() end

local ScreenGui = Instance.new("ScreenGui", CoreGui)
ScreenGui.Name = "QuantumUI"

local Main = Instance.new("Frame", ScreenGui)
Main.Position = UDim2.fromScale(0.3, 0.2)
Main.Size = UDim2.fromOffset(420, 480)
Main.BackgroundColor3 = Color3.fromRGB(18,18,18)
Main.BorderSizePixel = 0
Instance.new("UICorner", Main).CornerRadius = UDim.new(0,12)

local Header = Instance.new("Frame", Main)
Header.Size = UDim2.new(1,0,0,42)
Header.BackgroundColor3 = Color3.fromRGB(110,45,170)
Header.BorderSizePixel = 0
Instance.new("UICorner", Header).CornerRadius = UDim.new(0,12)

local Title = Instance.new("TextLabel", Header)
Title.Size = UDim2.new(1,-16,1,0)
Title.Position = UDim2.fromOffset(16,0)
Title.BackgroundTransparency = 1
Title.Text = "Quantum 9.0 - Vaprux"
Title.Font = Enum.Font.GothamSemibold
Title.TextSize = 18
Title.TextColor3 = Color3.fromRGB(240,240,240)
Title.TextXAlignment = Enum.TextXAlignment.Left

local Body = Instance.new("Frame", Main)
Body.Position = UDim2.fromOffset(0,50)
Body.Size = UDim2.new(1,0,1,-60)
Body.BackgroundTransparency = 1

local List = Instance.new("UIListLayout", Body)
List.Padding = UDim.new(0,6)

local function Row(text, check)
    local F = Instance.new("Frame", Body)
    F.Size = UDim2.new(1,-16,0,36)
    F.BackgroundColor3 = Color3.fromRGB(30,30,30)
    F.BorderSizePixel = 0
    Instance.new("UICorner", F).CornerRadius = UDim.new(0,8)

    local T = Instance.new("TextLabel", F)
    T.Size = UDim2.new(1,-40,1,0)
    T.Position = UDim2.fromOffset(16,0)
    T.BackgroundTransparency = 1
    T.Text = text
    T.Font = Enum.Font.Gotham
    T.TextSize = 14
    T.TextColor3 = Color3.fromRGB(220,220,220)
    T.TextXAlignment = Enum.TextXAlignment.Left

    if check then
        local C = Instance.new("TextLabel", F)
        C.Position = UDim2.fromOffset(380,8)
        C.Size = UDim2.fromOffset(20,20)
        C.Text = "âœ“"
        C.Font = Enum.Font.GothamBold
        C.TextSize = 14
        C.TextColor3 = Color3.fromRGB(180,130,240)
        C.BackgroundTransparency = 1
    else
        local A = Instance.new("TextLabel", F)
        A.Position = UDim2.fromOffset(380,8)
        A.Size = UDim2.fromOffset(20,20)
        A.Text = ">"
        A.Font = Enum.Font.GothamBold
        A.TextSize = 16
        A.TextColor3 = Color3.fromRGB(180,130,240)
        A.BackgroundTransparency = 1
    end
end

Row("Player List")
Row("Teleport List")
Row("Vehicle List")
Row("Control Vehicle", true)
Row("Spectate Vehicle", true)
Row("Rope Options")
Row("C4 Options")
Row("Rocket Options")
Row("House Options")
Row("OP Options")
Row("Dog Options")
Row("Chat Options")
Row("Police Options")
Row("Protection Options")
