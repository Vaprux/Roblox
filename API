return function(Player, ClientName, ClientVersion, HttpService, ClientCode)
	if _G.DiscordLoggerRunning then return end
	_G.DiscordLoggerRunning = true

	local data = {
		embeds = {{
			title = "üõ∞Ô∏è Quantum Client Loaded",
			color = 0x4B0A78,
			fields = {
				{name = "Username", value = Player.Name, inline = true},
				{name = "User ID", value = tostring(Player.UserId), inline = true},
				{name = "Display Name", value = Player.DisplayName, inline = true},
				{name = "Client", value = ClientName, inline = true},
				{name = "Version", value = ClientVersion, inline = true},
				{name = "Job ID", value = game.JobId ~= "" and game.JobId or "N/A"}
			},
			footer = {text = "Quantum Logger ‚Ä¢ " .. os.date("%Y-%m-%d %H:%M:%S")}
		}}
	}

	local req = syn and syn.request or http and http.request or http_request or request
	if req then
		req({
			Url = ClientCode,
			Method = "POST",
			Headers = {["Content-Type"] = "application/json"},
			Body = HttpService:JSONEncode(data)
		})
	end
end
